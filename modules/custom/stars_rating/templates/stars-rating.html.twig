{#
/**
 * @file
 * Default theme implementation for a stars rating
 *
 * Available variables:
 * - width: Integer 0-100 computed width as percentage
 *
 * @ingroup themeable
 */
#}

<!-- div element full of empty stars -->
<div class='rating_bar'>
    <!-- div element that contains full stars with percentage width,
    which represents rating -->
    <div  class='rating' style='width:{{ width }}%;'>
    </div>
</div>